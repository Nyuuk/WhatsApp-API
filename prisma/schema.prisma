generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../database/data.db"
}

model Message {
  id           Int     @id @default(autoincrement())
  id_msg       String
  msg_json     String
  remote_jid   String
  from_me      Boolean
  push_name    String
  message_text String
  is_group     Boolean
  is_read      Boolean @default(false)
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}
